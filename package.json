{
  "dependencies": {
    "@babel/core": "^7.21.4",
    "bcrypt": "^5.1.0",
    "connect-pg-simple": "^8.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.10.0",
    "pg-promise": "^11.4.3",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "ws": "^8.13.0"
  },
  "scripts": {
    "start": "node ./server.js",
    "start:dev": "concurrently \"npm:server:dev\" \"npm:build:dev\"",
    "server:dev": "NODE_ENV=development nodemon -e js,ejs --watch backend ./server.js",
    "build": "webpack",
    "build:dev": "webpack --watch",
    "prepare": "husky install",
    "db:create": "node-pg-migrate create -- ",
    "db:migrate": "node-pg-migrate up",
    "db:rollback": "node-pg-migrate down",
    "postinstall": "npm run db:migrate"
  },
  "devDependencies": {
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "connect-livereload": "^0.6.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.22",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "frontend/**/*.js": "webpack"
  }
}
